
{{#each rows}}
<div class="table-responsive">
<table class="table table-bordered border border-3 border-primary table-dark table-hover font-monospace">
    <thead class="table table-bordered border border-3 border-primary table-dark">
        <tr class="text-center">
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">E-mail</th>
        </tr>
    </thead>
    <tbody class="text-center">
        <tr class="text-success">
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.email}}</td>
        </tr>
    </tbody>
</table>
</div>

<div class="container row align-items-center text-center">
    <div class="col">
        <h1 style="color:greenyellow;"">Successfully Registered</h1>
    </div> 
</div>
{{/each}}